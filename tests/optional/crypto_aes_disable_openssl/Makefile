PROG=test_crypto_aes_disable_openssl
SRCS=../../crypto_aes/main.c crypto_aes.c
IDIRS=-I../../../cpusupport -I../../../crypto -I../../../util -I../../..
LDADD_REQ=-lcrypto
SUBDIR_DEPTH=../../..
RELATIVE_DIR=tests/optional/crypto_aes_disable_openssl
LIBALL=../../../liball/liball.a

all:
	if [ -z "$${HAVE_BUILD_FLAGS}" ]; then \
		cd ${SUBDIR_DEPTH}; \
		${MAKE} BUILD_SUBDIR=${RELATIVE_DIR} \
		    BUILD_TARGET=${PROG} buildsubdir; \
	else \
		${MAKE} ${PROG}; \
	fi

clean:
	rm -f ${PROG} ${SRCS:.c=.o}

${PROG}:${SRCS:.c=.o} ${LIBALL}
	${CC} -o ${PROG} ${SRCS:.c=.o} ${LIBALL} ${LDFLAGS} ${LDADD_EXTRA} ${LDADD_REQ} ${LDADD_POSIX}

../../crypto_aes/main.o: ../../crypto_aes/main.c ../../../cpusupport/cpusupport.h ../../../cpusupport-config.h ../../../crypto/crypto_aes.h ../../../util/getopt.h ../../../util/hexify.h ../../../util/insecure_memzero.h ../../../util/perftest.h ../../../util/warnp.h ../../../crypto/crypto_aes_aesni_m128i.h
	${CC} ${CFLAGS_POSIX} -D_POSIX_C_SOURCE=200809L -D_XOPEN_SOURCE=700 -DCPUSUPPORT_CONFIG_FILE=\"cpusupport-config.h\" ${CFLAGS_X86_SSE2} -I../../.. ${IDIRS} ${CPPFLAGS} ${CFLAGS} -c ../../crypto_aes/main.c -o ../../crypto_aes/main.o
crypto_aes.o: ../../../crypto/crypto_aes.c ../../../cpusupport/cpusupport.h ../../../cpusupport-config.h ../../../crypto/crypto_aes_aesni.h ../../../util/insecure_memzero.h ../../../util/warnp.h ../../../crypto/crypto_aes.h
	${CC} ${CFLAGS_POSIX} -D_POSIX_C_SOURCE=200809L -D_XOPEN_SOURCE=700 -DCPUSUPPORT_CONFIG_FILE=\"cpusupport-config.h\" -DDISABLE_OPENSSL -I../../.. ${IDIRS} ${CPPFLAGS} ${CFLAGS} -c ../../../crypto/crypto_aes.c -o crypto_aes.o

test:	all
	./test_crypto_aes -x

perftest:
	@${MAKE} all > /dev/null
	@printf "# nblks\tbsize\ttime\tspeed\n"
	@./test_crypto_aes -t |						\
	    grep "blocks" |						\
	    awk '{ printf "%i\t%i\t%.6f\t%.6f\n", $$1, $$5, $$6, $$8}'
